{
    "CAST": {
        "MRS_MY_NAME_IS": "miko",
        "MRS_CAST_ID": 1000
    },
    "MRS_ROS": {
        "MRS_ROS_DOMAIN_ID": 0,
        "MRS_ROS_LOCALHOST_ONLY": 0,
        "MRS_ROS_NAMESPACE": "/maid_robot_system/${MRS_MY_NAME_IS}",
        "MRS_ROS_OUTPUT_TYPE": "screen",
        "MRS_ROS_PACKAGE_FOLDER": "/opt/MaidRobotSystem/src/planetary_module/ros/package"
    },
    "MRS": {
        "MRS_WORKSPACE": "/opt/MaidRobotSystem",
        "MRS_SKIN_HOME": "/opt/MaidRobotSystem/data/skin/${MRS_MY_NAME_IS}"
    },
    "CMAKE": {
        "CMAKE_ARGS": "-DCMAKE_BUILD_TYPE=Debug"
    },
    "BUILD": {
        "BUILD_LOG_LEVEL": "DEBUG",
        "BUILD_PARALLEL_WORKERS": "$(($(nproc)/2+1))",
        "BUILD_HEAD_UNIT": "ON",
        "BUILD_ARM_UNIT": "ON",
        "BUILD_WAIST_DOWN_UNIT": "ON",
        "BUILD_MOBILITY_UNIT": "ON",
        "BUILD_CLOUD_UNIT": "ON",
        "BUILD_MANAGEMENT_UNIT": "ON",
        "BUILD_DEVELOP": "ON"
    }
}
