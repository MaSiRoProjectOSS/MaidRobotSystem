{
    "Description": {
        "USAGE": "",
        "CMD": "source /opt/MaidRobotSystem/src/planetary_module/ros/script/env.sh",
        "CATION": "This is not allowed in text that '(', ')'"
    },
    "CAST": {
        "MRS_CAST_NAME": "miko",
        "MRS_CAST_ID": 1000
    },
    "MRS": {
        "MRS_WORKSPACE": "/opt/MaidRobotSystem",
        "MRS_SKIN_HOME": "/opt/MaidRobotSystem/data/skin/${MRS_CAST_NAME}",
        "MRS_ROS_PACKAGE_FOLDER": "/opt/MaidRobotSystem/src/planetary_module/ros/package",
        "MRS_ROS_OUTPUT_TYPE": "screen",
        "MRS_ROS_NAMESPACE": "/maid_robot_system/${MRS_CAST_NAME}",
        "MRS_V4L_LEFT_BY_PATH": "",
        "MRS_V4L_LEFT_ID": 1,
        "MRS_V4L_RIGHT_BY_PATH": "",
        "MRS_V4L_RIGHT_ID": 3,
        "BUILD_HEAD_UNIT": "ON",
        "BUILD_ARM_UNIT": "ON",
        "BUILD_WAIST_DOWN_UNIT": "ON",
        "BUILD_MOBILITY_UNIT": "ON",
        "BUILD_CLOUD_UNIT": "ON",
        "BUILD_MANAGEMENT_UNIT": "ON",
        "BUILD_DEVELOP": "ON"
    },
    "ROS_VARIABLES": {
        "ROS_DISTRO": "humble",
        "ROS_DOMAIN_ID": 0,
        "ROS_LOCALHOST_ONLY": 0,
        "ROS_LOG_DIR": "${HOME}/.ros_log",
        "ROS_HOME": "${MRS_WORKSPACE}",
        "RCUTILS_CONSOLE_OUTPUT_FORMAT": "[{time}][{severity}] {message}",
        "RCUTILS_LOGGING_BUFFERED_STREAM": 1,
        "RCUTILS_COLORIZED_OUTPUT": 1,
        "RCUTILS_LOGGING_USE_STDOUT": 1
    },
    "BUILD": {
        "BUILD_CMAKE_ARGS": "-DCMAKE_BUILD_TYPE=Release",
        "BUILD_LOG_LEVEL": "RELEASE",
        "BUILD_PARALLEL_WORKERS": "$(($(nproc)/2+1))",
        "BUILD_COLCON_OPTION": "--symlink-install --parallel-workers ${BUILD_PARALLEL_WORKERS}"
    },
    "backup": {
        "RCUTILS_CONSOLE_OUTPUT_FORMAT": "[{severity} {time}] [{name}]: {message} {function_name} at {file_name}:{line_number}"
    }
}
